<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.web.WebView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.controller.HomeController">
    <!-- Label pour afficher le message de bienvenue -->
    <Label fx:id="welcomeLabel" layoutX="10.0" layoutY="10.0" style="-fx-font-size: 18px;" />

    <!-- WebView pour afficher la carte -->
    <WebView fx:id="mapWebView" layoutX="0.0" layoutY="40.0" AnchorPane.topAnchor="40.0" AnchorPane.bottomAnchor="0.0"
             AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" />
</AnchorPane>
